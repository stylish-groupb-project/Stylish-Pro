{
    "openapi": "3.0.0",
    "info": {
        "title": "API Doc",
        "description": "RESTful API for Stylish",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://13.55.47.107/api/1.0",
            "description": "Local server"
        }
    ],
    "paths": {
        "/test": {
            "get": {
                "summary": "just try",
                "description": "just try",
                "responses": {
                    "200": {
                        "description": "try success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/products/addProduct": {
            "post": {
                "summary": "Create a new product.",
                "description": "Create a new product with images and other information.",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "pictures": {
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        },
                                        "example": [
                                            "base64-encoded-image-1",
                                            "base64-encoded-image-2"
                                        ]
                                    },
                                    "data": {
                                        "type": "string",
                                        "format": "json",
                                        "example": "{\"category\":\"clothing\",\"title\":\"T-shirt\",\"description\":\"Comfortable cotton T-shirt\",\"price\":20.0,\"texture\":\"cotton\",\"wash\":\"machine wash\",\"place\":\"China\",\"note\":\"Handle with care\",\"story\":\"The story of the T-shirt\",\"colors\":[{\"code\":\"#FF0000\",\"name\":\"Red\"}],\"sizes\":[\"S\",\"M\",\"L\"],\"variants\":[{\"color_code\":\"#FF0000\",\"size\":\"M\",\"stock\":100}]}"
                                    }
                                },
                                "required": [
                                    "pictures",
                                    "data"
                                ]
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Success create product.",
                        "content": {
                            "text/plain": {
                                "schema": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/products/all": {
            "get": {
                "summary": "Get all products",
                "parameters": [
                    {
                        "name": "paging",
                        "in": "query",
                        "description": "Paging for request next page.",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Product"
                                            }
                                        },
                                        "next_paging": {
                                            "type": "number",
                                            "description": "Next page number. If there are no more pages, server won't return next_paging parameter."
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "ProductInput": {
                "type": "object",
                "properties": {
                    "category": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "price": {
                        "type": "number"
                    },
                    "texture": {
                        "type": "string"
                    },
                    "wash": {
                        "type": "string"
                    },
                    "place": {
                        "type": "string"
                    },
                    "note": {
                        "type": "string"
                    },
                    "story": {
                        "type": "string"
                    },
                    "colors": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Color"
                        }
                    },
                    "sizes": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "variants": {
                        "type": "array",
                        "items": {
                            "$ref": "#/components/schemas/Variant"
                        }
                    }
                },
                "required": [
                    "category",
                    "title",
                    "description",
                    "price",
                    "texture",
                    "wash",
                    "place",
                    "note",
                    "story",
                    "colors",
                    "sizes",
                    "variants"
                ]
            },
            "Variant": {
                "type": "object",
                "properties": {
                    "color_code": {
                        "type": "string"
                    },
                    "size": {
                        "type": "string"
                    },
                    "stock": {
                        "type": "integer"
                    }
                },
                "required": [
                    "color_code",
                    "size",
                    "stock"
                ]
            },
            "Color": {
                "type": "object",
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "code",
                    "name"
                ]
            },
            "Product": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number",
                        "description": "Product ID"
                    },
                    "category": {
                        "type": "string",
                        "description": "Product category"
                    },
                    "title": {
                        "type": "string",
                        "description": "Product title"
                    },
                    "description": {
                        "type": "string",
                        "description": "Product description"
                    },
                    "price": {
                        "type": "number",
                        "description": "Product price"
                    },
                    "texture": {
                        "type": "string",
                        "description": "Product texture"
                    },
                    "wash": {
                        "type": "string",
                        "description": "Washing instructions"
                    },
                    "place": {
                        "type": "string",
                        "description": "Place of origin"
                    },
                    "note": {
                        "type": "string",
                        "description": "Product notes"
                    },
                    "story": {
                        "type": "string",
                        "description": "Product story"
                    },
                    "colors": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "code": {
                                    "type": "string",
                                    "description": "Color code"
                                },
                                "name": {
                                    "type": "string",
                                    "description": "Color name"
                                }
                            }
                        },
                        "description": "Array of colors"
                    },
                    "sizes": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "Available sizes"
                    },
                    "variants": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "color_code": {
                                    "type": "string",
                                    "description": "Color code for the variant"
                                },
                                "size": {
                                    "type": "string",
                                    "description": "Size of the variant"
                                },
                                "stock": {
                                    "type": "number",
                                    "description": "Stock quantity"
                                }
                            }
                        },
                        "description": "Array of variants"
                    },
                    "main_image": {
                        "type": "string",
                        "description": "URL of the main image"
                    },
                    "images": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "description": "Array of image URLs"
                    }
                }
            }
        }
    }
}